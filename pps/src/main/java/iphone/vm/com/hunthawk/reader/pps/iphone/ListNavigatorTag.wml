#if(${isJump})
<input name="gotoPage" title="goto" format="*N" size="5" maxlength="5" value="1"/>
<anchor>${title}
<go href="${url}" method="post">
	 <postfield name="pn" value="$goto" />
</go>
</anchor>
#else
#if(${total}>1)
		#if(${flag}==1)
				  #if(${current}!=1)
				  	<a href="${prelink}">上一页</a>
				  #end
				  #if(${current}<${total})
				  	<a href="${nextlink}">下一页</a>
				  #end
		#else
			<a href="${url}">${title}</a>
		#end
	#end
#end